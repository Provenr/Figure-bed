extends layout

block content
  h1= title
  p Welcome to #{title}
  ul
    each val, index in list
      li
        img(src= val.Url)
  form(
    action='/uploads'
    method='POST'
    enctype="multipart/form-data"
  )
    input(
      type= 'file'
      name= 'file'
    )
    input(
      type= 'text'
      name= 'fileName'
    )
    input(
      type="submit"
      value="submit"
    )
  button(onClick='uploaTtest()') 上传

  script(src='/javascripts/index.js')
